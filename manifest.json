{
"manifest_version": 3,
    "name": "block KT Spy",
    "description": "block KT spy",
    "version": "0.3.6",
    "background": {
        "service_worker": "redirect.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
      "48": "blockspy.png"
    },    
    "options_page": "options.html",
    "permissions": [
      "storage", "activeTab"
    ]
}